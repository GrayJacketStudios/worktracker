<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sebastian.cl.worktracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".registros.EditRegistroActivity"></activity>
        <activity android:name=".trabajos.EditTrabajo" />
        <activity android:name=".usuarios.RegistroActivity" />
        <activity android:name=".trabajos.CreateTrabajo" />
        <activity android:name=".registros.CreateRegistro" />
        <activity android:name=".registros.RegistrosList" />
        <activity android:name=".trabajos.TrabajosList" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>